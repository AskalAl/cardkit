{
  "name": "cardkit",
  "version": "2.0.6",
  "description":
    "A simple, powerful and fully configurable image editor for web browsers and servers. Optional UI included.",
  "main": "cardkit.js",
  "homepage": "https://times.github.io/cardkit",
  "bugs": {
    "url": "https://github.com/times/cardkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/times/cardkit.git"
  },
  "scripts": {
    "test":
      "istanbul cover --dir test/coverage _mocha -- --require babel-core/register --require ignore-styles",
    "jsdoc":
      "jsdoc -c ./jsdoc.config.json -d ./docs/code -t ./node_modules/minami --verbose -R ./README.md",
    "demo": "webpack --env=dist-docs",
    "docs": "npm run jsdoc; npm run demo",
    "start": "webpack-dev-server -d --content-base ./src --hot --open",
    "dist": "npm run build",
    "build":
      "echo \"[NOTICE] This task will build ALL CardKit versions\"; echo \"[LOG] Building DOM library\"; npm run build-dom; echo \"[LOG] Building server library\"; npm run build-server; echo \"[LOG] Building core library\"; npm run build-core; echo \"[LOG] Building DOM <script> library\"; npm run build-dom-script; echo \"[LOG] Building core <script> library\"; npm run build-core-script",
    "build-dom": "webpack --env=dist-dom",
    "build-server": "webpack --env=dist-server",
    "build-core": "webpack --env=dist-core",
    "build-dom-script": "webpack --env=dist-dom-script",
    "build-core-script": "webpack --env=dist-core-script"
  },
  "author": "Chris Hutchinson <chris.hutchinson@thetimes.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.1.2",
    "css-loader": "^0.25.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.2",
    "jsdom": "^9.5.0",
    "lodash": "^4.16.3",
    "minami": "^1.1.1",
    "minimist": "^1.2.0",
    "mocha": "^5.0.1",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "sinon": "^4.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "deep-extend": "^0.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-color": "^2.3.2",
    "react-dom": "^16.2.0",
    "rvg.js": "^1.1.0",
    "svg2png": "4.0.0"
  }
}
